{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "sdh-ai-admin",
  "builds": [
    {
      "src": "admin-panel/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "admin-panel/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  }
}